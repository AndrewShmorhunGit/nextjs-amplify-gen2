{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "target": "es2022",
    "module": "es2022",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "$amplify/*": [".amplify/generated/*"]
    }
  },
  "include": ["./**/*.ts"]
}
